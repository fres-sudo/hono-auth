    "drizzle-orm": "^0.35.3",
    "drizzle-zod": "^0.5.1",
    "handlebars": "^4.7.8",
    "hono": "^4.6.3",
    "hono-rate-limiter": "^0.4.0",
    "nodemailer": "^6.9.16",
    "oslo": "^1.2.1",
    "postgres": "^3.4.5",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.8.0",
    "typescript": "^5.6.3",
    "zod": "^3.23.8",
    "zod-validator": "^1.0.1"
  },
